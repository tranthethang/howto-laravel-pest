{
    "preset": "laravel",
    "rules": {
        "concat_space": true,
        "ternary_to_null_coalescing": true,
        "nullable_type_declaration": true,
        "yoda_style": true,
        "nullable_type_declaration_for_default_null_value": true
    },
    "exclude": [
        "public",
        "bootstrap"
    ]
}
